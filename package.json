{
	"name": "briki-extension",
	"publisher": "meteca",
	"displayName": "Briki extension",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "briki",
					"title": "Briki",
					"icon": "images/natale.svg"
				}
			]
		},
		"views": {
			"platformio": [
				{
					"id": "briki-commands",
					"name": "Briki",
					"when": "pioCoreReady"
				}
			],
			"briki": [
				{
					"id": "commands",
					"name": "Commands"
				},
				{
					"id": "prova",
					"name": "prova"
				}
			]
		},
		"commands": [
			{
				"command": "briki.brikiOta",
				"title": "Briki OTA"
			},
			{
				"command": "briki.partition",
				"title": "Partition"
			},
			{
				"command": "briki.documentation",
				"title": "Documentation"
			}
		],
		"themes": [
			{
				"label": "briki",
				"uiTheme": "vs-dark",
				"path": "./themes/briki-color-theme.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"extensionDependencies": [
		"platformio.platformio-ide"
	],
	"dependencies": {
		"@types/ini": "^1.3.30",
		"express": "^4.17.1",
		"fs": "0.0.1-security",
		"ini": "^1.3.5",
		"path": "^0.12.7",
		"csv-parser": "*"
	}
}
